        {
          "Name": "Estoque API",
          "Version": "v1",
          "Url": "http://localhost:5000/swagger/estoque/swagger.json"
        }
      ]
    },
    {
      "Key": "vendas",
      "Config": [
        {
          "Name": "Vendas API",
          "Version": "v1",
          "Url": "http://localhost:5000/swagger/vendas/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
